  "glossary": {
    "National Health Expenditures": {
      "definition": "Total annual spending for healthcare goods and services, public health activities, government administration, net cost of health insurance, and investment related to healthcare",
      "sources": [
        {
          "organization": "CMS",
          "title": "National Health Expenditure Accounts Methodology Paper",
          "url": "https://www.cms.gov/files/document/definitions-sources-and-methods.pdf",
          "publicationDate": "2023-01-01",
          "accessDate": "2025-10-18",
          "dataYear": 2023
        }
      ]
    },
    "Out-of-Pocket": {
      "definition": "Direct spending by consumers for healthcare goods and services, including deductibles, coinsurance, and copayments. Does not include health insurance premiums",
      "sources": [
        {
          "organization": "CMS",
          "title": "Quick Definitions for NHEA Categories",
          "url": "https://www.cms.gov/files/document/quick-definitions-national-health-expenditures-accounts-nhea-categories.pdf",
          "publicationDate": "2023-01-01",
          "accessDate": "2025-10-18",
          "dataYear": 2023
        }
      ]
    }
  },
  "methodology": {
    "dataCollection": "Data collected systematically from tier-1 government sources (CMS, CDC, Census Bureau) and tier-2 research organizations (KFF, Milbank Memorial Fund). All data points cross-referenced with multiple sources where possible.",
    "categorization": "Follows official CMS National Health Expenditure Accounts (NHEA) categorization methodology. Additional subcategories created based on research literature and clinical practice patterns.",
    "limitations": [
      "Some subcategories estimated from percentage allocations due to lack of direct CMS reporting",
      "State-level data not available for all categories",
      "Demographic breakdowns limited to categories where CMS publishes age-specific data",
      "2023 data finalized in December 2024; minor revisions possible in future CMS updates"
    ],
